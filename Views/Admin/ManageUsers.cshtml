@model IEnumerable<EliteSportsAcademy.Models.Account.ApplicationUser>


@{
    Layout = "~/Views/Shared/DashBoard/_AdminLayout.cshtml";
    ViewData["Title"] = "Manage Users";
}

<h2>Manage Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.UserName</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
            </tr>
        }
    </tbody>
</table>
